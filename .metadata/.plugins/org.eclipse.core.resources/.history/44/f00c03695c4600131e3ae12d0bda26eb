object Problem4 {
	val biggest3numbersPalindrome = (100 to 999).view.flatMap(i => (i to 999).map(i *)).filter(n => n.toString == n.toString.reverse).max
                                                  //> biggest3numbersPalindrome  : Int = 906609
 
 def divisors(xs: List[Int]): List[(Int, Int)] =
		xs.flatMap(x =>
		xs.filter(y => y != x && y % x == 0).map((x, _)))
                                                  //> divisors: (xs: List[Int])List[(Int, Int)]
 divisors(List(1,2,3,4,5,6,7))                    //> res0: List[(Int, Int)] = List((1,2), (1,3), (1,4), (1,5), (1,6), (1,7), (2,4
                                                  //| ), (2,6), (3,6))
}