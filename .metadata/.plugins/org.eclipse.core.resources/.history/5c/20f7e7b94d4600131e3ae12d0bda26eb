object Problem4 {
	val biggest3numbersPalindrome = (100 to 999).view.flatMap(i => (i to 999).map(i *)).filter(n => n.toString == n.toString.reverse)
                                                  //> biggest3numbersPalindrome  : scala.collection.SeqView[Int,Seq[_]] = SeqViewN
                                                  //| F(...)
}